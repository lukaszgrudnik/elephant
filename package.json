{
  "name": "elephant",
  "workspaces": [
    "apps/shell"
  ],
  "scripts": {
    "serve:shell": "npm run serve --workspace=apps/shell",
    "build:shell": "npm run build --workspace=apps/shell",
    "build:shell:mobile": "npm run build --workspace=apps/shell && npx cap copy",
    "open:shell:android": "npm run build --workspace=apps/shell && npx cap copy && npx cap add android && npx cap open android",
    "open:shell:ios": "npm run build --workspace=apps/shell && npx cap copy && npx cap add ios && npx cap open ios",
    "test": "jest"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@rsbuild/core": "^1.6.2",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@swc/core": "^1.15.0",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "@capacitor/ios": "^7.4.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
