{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.17.0",
    "generators": {
      "custom": {
        "inputSpec": "./openapi/openapi.yaml",
        "output": "api",
        "generatorName": "typescript-axios",
        "additionalProperties": {
          "npmName": "elephant.api",
          "npmVersion": "1.0.0",
          "npmDescription": "API client for Elephant service",
          "supportES6": true
        }
      }
    }
  }
}
